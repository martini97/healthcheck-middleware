{
  "name": "@martini97/healthcheck-middleware",
  "version": "0.0.1",
  "description": "Middleware to check if the service dependencies are up",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "rimraf lib dist coverage",
    "prepublish": "run-s clean build",
    "lint": "eslint --ext js --ext md .",
    "pretest": "npm run build",
    "test": "ava --verbose"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.25.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "ava-spec": "^1.1.0",
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-concise": "^0.6.1",
    "eslint-config-concise-esnext": "^0.6.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-eslint-comments": "^1.0.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mysticatea": "^4.2.2",
    "eslint-plugin-node": "^5.1.1",
    "express": "^4.15.4",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0"
  },
  "repository": "martini97/healthcheck-middleware",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "@ava/stage-4",
        "stage-3"
      ]
    }
  }
}
