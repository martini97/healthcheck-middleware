{
  "name": "services-healthcheck",
  "version": "0.3.4",
  "description": "Middleware to check if the service dependencies are up",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "rimraf lib dist coverage",
    "prepublish": "run-s clean build",
    "removeSkip": "sed -i.bak 's/.skip//' test/*",
    "pretest": "npm run build",
    "lint": "eslint .",
    "test": "eslint . && ava test/*.spec.js --verbose"
  },
  "dependencies": {
    "amqp": "^0.2.6",
    "axios": "^0.16.2"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "ava-spec": "^1.1.0",
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-import-export": "^1.0.2",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "express": "^4.15.4",
    "knex": "^0.13.0",
    "mock-knex": "^0.3.9",
    "mysql": "^2.14.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "supertest": "^3.0.0"
  },
  "repository": "martini97/services-healthcheck",
  "keywords": [
    "healthcheck",
    "axios",
    "services",
    "amqp",
    "middleware",
    "express"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "ava": {
    "presets": [
      "es2016",
      "stage-2",
      "import-export"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
